{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@assets": ["assets"],
      "@assets/*": ["assets/*"],

      "@components": ["components"],
      "@components/*": ["components/*"],

      "@core": ["core"],
      "@core/*": ["core/*"],

      "@helpers": ["helpers"],
      "@helpers/*": ["helpers/*"],

      "@html": ["html"],
      "@html/*": ["html/*"],

      "@http": ["http"],
      "@http/*": ["http/*"],

      "@redux": ["redux"],
      "@redux/*": ["redux/*"],

      "@styles": ["styles"],
      "@styles/*": ["styles/*"],

      "@types": ["types"],
      "@types/*": ["types/*"],

      "@utils": ["utils"],
      "@utils/*": ["utils/*"],

      "@wquery": ["wquery"],
      "@wquery/*": ["wquery/*"],

      "@pages": ["pages"],
      "@pages/*": ["pages/*"],

      "@routing": ["routing"],
      "@routing/*": ["routing/*"],
    }
  }
}